class student:
    def __init__(self, name, grade):
        self.name = name
        self.grade = grade

    def introduce(self):
        print("Hi my name is ", self.name, "and I am in grade ", self.grade)

student1 = student("Fat Albert", 9)
student2 = student("Stinky John", 12)

student1.introduce()
student2.introduce()
